
{
  "manifest_version": 3,
  "name": "TimeStampTube",
  "description": "Base Level Extension",
  "permissions": ["storage", "tabs"],
  "host_permissions": ["https://*.youtube.com/*"],
  "version": "1.0",
  "action": {
    "default_popup": "hello.html",
    "default_icon": "icons/chromeIcon.png"
  },
  "commands": {
    "_execute_action": {
      "suggested_key": {
        "default": "Ctrl+B",
        "mac": "Command+B"
      }
    }
  }
}
